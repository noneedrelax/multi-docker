{
	"AWSEBDockerrunVersion": 2, 
	"containerDefinitions": [
		{
			"name": "client", 
			"image": "563421743711.dkr.ecr.us-west-2.amazonaws.com/multi-client", 
			"hostname", "client", 
			"essential": false, 
			"memory": 128
		}, 
		{
			"name": "server", 
			"image": "563421743711.dkr.ecr.us-west-2.amazonaws.com/multi-server", 
			"hostname", "api", 
			"essential": false, 
			"memory": 128
		}, 
		{
			"name": "worker", 
			"image": "563421743711.dkr.ecr.us-west-2.amazonaws.com/multi-worker", 
			"hostname", "work", 
			"essential": false, 
			"memory": 128
		}, 
		{
			"name": "nginx", 
			"image": "563421743711.dkr.ecr.us-west-2.amazonaws.com/multi-nginx", 
			"hostname", "nginx", 
			"essential": true, 
			"portMappings": [
				"hostPort": 80, 
				"containerPort": 80
			], 
			"links": ["client", "server"], 
			"memory": 128
		}
	]
}

